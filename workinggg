local function PBUM_fake_script() -- OPEN.LocalScript 
	local script = Instance.new('LocalScript', OPEN)

	local brick = script.Parent.Parent.Frame2
	
	script.Parent.MouseButton1Click:Connect(function()
		wait(1.3)
		while brick.BackgroundTransparency > 0.01 do
			wait(0.1)
			brick.JAILBREAK.TextTransparency = brick.JAILBREAK.TextTransparency - 0.05
			brick.JAILBREAK.BackgroundTransparency = brick.JAILBREAK.BackgroundTransparency - 0.05
			brick.MURDER.TextTransparency = brick.MURDER.TextTransparency - 0.05
			brick.MURDER.BackgroundTransparency = brick.MURDER.BackgroundTransparency - 0.05
			brick.DAHOOD.BackgroundTransparency = brick.DAHOOD.BackgroundTransparency - 0.05
			brick.DAHOOD.TextTransparency = brick.DAHOOD.TextTransparency -0.05
			brick.Parent.OPEN.TextTransparency = brick.Parent.OPEN.TextTransparency + 0.05
			brick.Parent.OPEN.BackgroundTransparency = brick.Parent.OPEN.BackgroundTransparency + 0.05
			brick.XX.TextTransparency = brick.XX.TextTransparency - 0.05
			brick.XX.BackgroundTransparency = brick.XX.BackgroundTransparency - 0.05
			brick.VERSION.TextTransparency = brick.VERSION.TextTransparency - 0.05
			brick.VERSION.BackgroundTransparency = brick.VERSION.BackgroundTransparency - 0.05
			brick.BEAUTIFUL.TextTransparency = brick.BEAUTIFUL.TextTransparency - 0.05
			brick.BEAUTIFUL.BackgroundTransparency = brick.BEAUTIFUL.BackgroundTransparency - 0.05
			brick.BLOXBURG.TextTransparency = brick.BLOXBURG.TextTransparency - 0.05
			brick.BLOXBURG.BackgroundTransparency = brick.BLOXBURG.BackgroundTransparency - 0.05
			brick.BackgroundTransparency = brick.BackgroundTransparency - 0.05
		end
		brick.nextpage.Visible = true
		brick.ImageButton.Visible = true
	end)
	
	
	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(PBUM_fake_script)()
