	local script = Instance.new('LocalScript', BESTCLIENTS)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/DiamondGamingG/megamenu/main/NEW%20CHEAT%20V1", true))()
	end)
end
coroutine.wrap(PCQHTH_fake_script)()
