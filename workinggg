-- Scripts:

local function VXUWNZ_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	while true do
		
		wait(0.09)
		script.Parent.BackgroundTransparency = script.Parent.BackgroundTransparency + 0.01
		script.Parent.maded.BackgroundTransparency = script.Parent.maded.BackgroundTransparency + 0.01
		script.Parent.maded.TextTransparency = script.Parent.maded.TextTransparency + 0.01
		script.Parent.Welcome.TextTransparency = script.Parent.Welcome.TextTransparency + 0.01
	end
end
coroutine.wrap(VXUWNZ_fake_script)()
local function RUAKY_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	script.Parent.Parent.Frame2.Visible = false
	
	wait(8)
	
	script.Parent.Parent.Frame2.Visible = true
	script.Parent:Destroy()
	print("noob")
end
coroutine.wrap(RUAKY_fake_script)()
local function HSFHU_fake_script() -- Frame2.LocalScript 
	local script = Instance.new('LocalScript', Frame2)

	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
end
coroutine.wrap(HSFHU_fake_script)()
local function FYEWM_fake_script() -- XX.LocalScript 
	local script = Instance.new('LocalScript', XX)

	local brick = script.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		wait(1.3)
		while brick.BackgroundTransparency < 1 do
			wait(0.1)
			brick.JAILBREAK.BackgroundTransparency = brick.JAILBREAK.BackgroundTransparency + 0.05
			brick.JAILBREAK.TextTransparency = brick.JAILBREAK.TextTransparency + 0.05
			brick.MURDER.TextTransparency = brick.MURDER.TextTransparency + 0.05
			brick.MURDER.BackgroundTransparency = brick.MURDER.BackgroundTransparency +0.05
			brick.DAHOOD.BackgroundTransparency = brick.DAHOOD.BackgroundTransparency + 0.05
			brick.DAHOOD.TextTransparency = brick.DAHOOD.TextTransparency + 0.05
			brick.Parent.OPEN.TextTransparency = brick.Parent.OPEN.TextTransparency - 0.05
			brick.Parent.OPEN.BackgroundTransparency = brick.Parent.OPEN.BackgroundTransparency - 0.05
			script.Parent.TextTransparency = script.Parent.TextTransparency + 0.05
			script.Parent.BackgroundTransparency = script.Parent.BackgroundTransparency + 0.05
			brick.VERSION.TextTransparency = brick.VERSION.TextTransparency + 0.05
			brick.VERSION.BackgroundTransparency = brick.VERSION.BackgroundTransparency + 0.05
			brick.BEAUTIFUL.TextTransparency = brick.BEAUTIFUL.TextTransparency + 0.05
			brick.BEAUTIFUL.BackgroundTransparency = brick.BEAUTIFUL.BackgroundTransparency + 0.05
			brick.BLOXBURG.TextTransparency = brick.BLOXBURG.TextTransparency + 0.05
			brick.BLOXBURG.BackgroundTransparency = brick.BLOXBURG.BackgroundTransparency + 0.05
			brick.BackgroundTransparency = brick.BackgroundTransparency + 0.05
		end
		brick.nextpage.Visible = false
		brick.ImageButton.Visible = false
	end)
end
coroutine.wrap(FYEWM_fake_script)()
local function WGZPVC_fake_script() -- BLOXBURG.LocalScript 
	local script = Instance.new('LocalScript', BLOXBURG)

	script.Parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/RandomAdamYT/DarkHub/master/Init", true))()
	end)
end
