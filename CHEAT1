-- Scripts:

local function XCKETH_fake_script() -- OPEN.LocalScript 
	local script = Instance.new('LocalScript', OPEN)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Visible = false
		script.Parent.Parent.page1.Visible = true
	end)
	
	button = script.Parent
	button.Draggable = true
	button.Active = true
	button.Selectable = true
end
coroutine.wrap(XCKETH_fake_script)()
local function PHKNU_fake_script() -- Frame1.LocalScript 
	local script = Instance.new('LocalScript', Frame1)

	script.Parent.Visible = true
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.1
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.2
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.3
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.4
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.5
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.6
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.7
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.8
	wait(0.4)
	script.Parent.BackgroundTransparency = 0.9
	wait(0.4)
	script.Parent.Visible = false
end
coroutine.wrap(PHKNU_fake_script)()
local function AAULFTN_fake_script() -- infinitejump.LocalScript 
	local script = Instance.new('LocalScript', infinitejump)

	-- by isaraw8912
	-- Press [R] to turn off and to turn on
	
	_G.infinjump = true
	
	local Player = game:GetService("Players").LocalPlayer
	local Mouse = Player:GetMouse()
	Mouse.KeyDown:connect(function(k)
		if _G.infinjump then
			if k:byte() == 32 then
				Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
				Humanoid:ChangeState("Jumping")
				wait(0.0000000001)
				Humanoid:ChangeState("Seated")
			end
		end
	end)
	
	local Player = game:GetService("Players").LocalPlayer
	local Mouse = Player:GetMouse()
	Mouse.KeyDown:connect(function(k)
		k = k:lower()
		if k == "r" then
			if _G.infinjump == true then
				_G.infinjump = false
			else
				_G.infinjump = true
			end
		end
	end)
end
