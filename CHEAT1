coroutine.wrap(VVNK_fake_script)()
local function VHVY_fake_script() -- page2.LocalScript 
	local script = Instance.new('LocalScript', page2)

	frame = script.Parent
	frame.Draggable = true
	frame.Active = true
	frame.Selectable = true
	frame.Visible = false
end
coroutine.wrap(VHVY_fake_script)()
local function MOZRKR_fake_script() -- nextpage3.LocalScript 
	local script = Instance.new('LocalScript', nextpage3)

	
end
coroutine.wrap(MOZRKR_fake_script)()
local function STKKD_fake_script() -- pageprecedent.LocalScript 
	local script = Instance.new('LocalScript', pageprecedent)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.page1.Visible = true
		
	end)
end
coroutine.wrap(STKKD_fake_script)()
local function RLJQP_fake_script() -- Close2.LocalScript 
	local script = Instance.new('LocalScript', Close2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
		wait(0.2)
		script.Parent.Parent.Parent.OPEN.Visible = true
	end)
end
coroutine.wrap(RLJQP_fake_script)()
local function OSOO_fake_script() -- BTOOLS.LocalScript 
	local script = Instance.new('LocalScript', BTOOLS)

	script.Parent.MouseButton1Click:Connect(function()
		-- Made By : Bullet-X-Proof
	
	
		local Btools = Instance.new("ScreenGui")
		local Frame = Instance.new("Frame")
		local Button = Instance.new("TextButton")
		local TextLabel = Instance.new("TextLabel")
		local TextButton = Instance.new("TextButton")
	
		Btools.Name = "Btools"
		Btools.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	
		Frame.Parent = Btools
		Frame.BackgroundColor3 = Color3.fromRGB(13, 21, 21)
		Frame.Position = UDim2.new(0.00999999978, 0, 1, 0)
		Frame.Size = UDim2.new(0, 103, 0, 67)
	
		Button.Name = "Button"
		Button.Parent = Frame
		Button.BackgroundColor3 = Color3.fromRGB(39, 61, 77)
		Button.BorderColor3 = Color3.fromRGB(27, 42, 53)
		Button.BorderSizePixel = 5
		Button.Position = UDim2.new(-1.1920929e-07, 0, 0.625074625, 0)
		Button.Size = UDim2.new(0, 103, 0, 25)
		Button.Font = Enum.Font.GothamBlack
		Button.Text = "BTools"
		Button.TextColor3 = Color3.fromRGB(0, 0, 0)
		Button.TextSize = 25.000
		Button.MouseButton1Down:connect(function()
			a = Instance.new("HopperBin", game.Players.LocalPlayer.Backpack)
			a.BinType = 2
			b = Instance.new("HopperBin", game.Players.LocalPlayer.Backpack)
			b.BinType = 3
			c = Instance.new("HopperBin", game.Players.LocalPlayer.Backpack)
			c.BinType = 4
		end)
	
		TextLabel.Parent = Frame
		TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.BackgroundTransparency = 1.000
		TextLabel.Position = UDim2.new(0.00970873795, 0, 0.134328365, 0)
		TextLabel.Size = UDim2.new(0, 102, 0, 20)
		TextLabel.Font = Enum.Font.GothamBlack
		TextLabel.Text = "By : Bullet-X-Proof"
		TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextLabel.TextSize = 10.000
		TextLabel.TextStrokeColor3 = Color3.fromRGB(85, 170, 255)
		TextLabel.TextStrokeTransparency = 3.000
	
		TextButton.Parent = Frame
		TextButton.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
		TextButton.Position = UDim2.new(0, 0, -0.208955228, 0)
		TextButton.Size = UDim2.new(0, 103, 0, 14)
		TextButton.Font = Enum.Font.GothamBlack
		TextButton.Text = "?"
		TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
		TextButton.TextSize = 22.000
	
		-- Scripts:
	
		local function PZTHXOV_fake_script() -- TextButton.Script 
			local script = Instance.new('Script', TextButton)
	
			wait(1)
	
			local T = script.Parent
			local on=false
			local f=script.Parent.Parent.Parent.Frame
	
			function click()
	
				if on==false then
	
					on=true
	
					f:TweenPosition(UDim2.new(0.01, 0,0.87, 0),"Out","Sine",0.5) --Change to the position your gui will be when its open!
	
					T.Text = "V"
	
				else
	
					on=false
	
					f:TweenPosition(UDim2.new(0.01, 0,1, 0),"Out","Sine",0.5) --Change to the position your gui will be when its closed!
	
					T.Text = "?"
	
				end
	
			end
	
	
	
			script.Parent.MouseButton1Click:connect(click)
		end
		coroutine.wrap(PZTHXOV_fake_script)()
	end)
end
coroutine.wrap(OSOO_fake_script)()
local function ATVLF_fake_script() -- COLOURS.LocalScript 
	local script = Instance.new('LocalScript', COLOURS)

	local frame = script.Parent
	
	while true do
		for hue = 0, 255, 4 do
			-- HSV = hue, saturation, value
			-- If we loop from 0 to 1 repeatedly, we get a rainbow!
			frame.BorderColor3 = Color3.fromHSV(hue/256, 1, 1)
			frame.BackgroundColor3 = Color3.fromHSV(hue/256, .5, .8)
			wait()
		end
	end
end
coroutine.wrap(ATVLF_fake_script)()
local function DDYSP_fake_script() -- COLOURS.LocalScript2 
	local script = Instance.new('LocalScript', COLOURS)

	script.Parent.MouseButton1Click:Connect(function()
		local frame = script.Parent.Parent
		local frame2 = script.Parent.Parent.Parent.page1
		Hello = true
	
		while Hello do
			for hue = 0, 255, 4 do
				-- HSV = hue, saturation, value
				-- If we loop from 0 to 1 repeatedly, we get a rainbow!
				frame.BorderColor3 = Color3.fromHSV(hue/256, 1, 1)
				frame.BackgroundColor3 = Color3.fromHSV(hue/256, .5, .8)
				frame2.BackgroundColor3 = Color3.fromHSV(hue/256, 1, 1)
				frame2.BackgroundColor3 = Color3.fromHSV(hue/256,.5, .8)
				wait()
			end
		end
	end)
	
	script.Parent.Parent.NOCOLOURS.MouseButton1Click:Connect(function()
		Hello = false
		wait(5)
		script.Parent.Parent.Parent.page1.BackgroundColor3(255, 0, 4)
		script.Parent.Parent.Parent.page2.BackgroundColor3(255, 0, 4)
	end)
end
coroutine.wrap(DDYSP_fake_script)()
local function ATZPG_fake_script() -- tptoplayer.LocalScript 
	local script = Instance.new('LocalScript', tptoplayer)

	script.Parent.MouseButton1Click:Connect(function()
		-- ~ Can only be executed as a local script ~ --
	
		script.Name = "TeleportGUI"
		print("Thank you for using LoveingLiamGuy's Teleport GUI!")
		print("You cannot use this on yourself.")
		print("If you die, the GUI will be gone, so reactivate the script.")
		print("Type the name of the player to teleport to them/teleport them to you.")
		print("Have fun!")
		print("~LoveingLiamGuy")
	
		local player = script.Parent.Parent
		local playerSelected = 0
		local playerSelected2 = 0
	
		local gui = Instance.new("ScreenGui", player.PlayerGui)
		gui.ResetOnSpawn = false
		gui.Name = "TeleportGui"
		local button = Instance.new("TextButton", gui)
		button.AutoButtonColor = true
		button.Position = UDim2.new(0, 0, 0.25, 0)
		button.Size = UDim2.new(0.15, 0, 0.05, 0)
		button.Text = "Teleport To Player"
		button.Font = Enum.Font.SourceSans
		button.TextScaled = true
		button.TextSize = 100
		button.BorderSizePixel = 0
		button.BorderColor3 = Color3.new(255, 255, 255)
		button.BackgroundColor3 = Color3.new(255, 255, 255)
		button.Name = "TeleportButton"
		local textbox = Instance.new("TextBox", gui)
		textbox.Position = UDim2.new(0.175, 0, 0.25, 0)
		textbox.Size = UDim2.new(0.15, 0, 0.05, 0)
		textbox.Text = ""
		textbox.Font = Enum.Font.SourceSans
		textbox.TextScaled = true
		textbox.TextSize = 100
		textbox.BorderSizePixel = 0
		textbox.BorderColor3 = Color3.new(255, 255, 255)
		textbox.BackgroundColor3 = Color3.new(255, 255, 255)
		textbox.Name = "TeleportAnswer"
		local button2 = Instance.new("TextButton", gui)
		button2.AutoButtonColor = true
		button2.Position = UDim2.new(0, 0, 0.35, 0)
		button2.Size = UDim2.new(0.15, 0, 0.05, 0)
		button2.Text = "Teleport Player To You"
		button2.Font = Enum.Font.SourceSans
		button2.TextScaled = true
		button2.TextSize = 100
		button2.BorderSizePixel = 0
		button2.BorderColor3 = Color3.new(255, 255, 255)
		button2.BackgroundColor3 = Color3.new(255, 255, 255)
		button2.Name = "TeleportButton2"
		local textbox2 = Instance.new("TextBox", gui)
		textbox2.Position = UDim2.new(0.175, 0, 0.35, 0)
		textbox2.Size = UDim2.new(0.15, 0, 0.05, 0)
		textbox2.Text = ""
		textbox2.Font = Enum.Font.SourceSans
		textbox2.TextScaled = true
		textbox2.TextSize = 100
		textbox2.BorderSizePixel = 0
		textbox2.BorderColor3 = Color3.new(255, 255, 255)
		textbox2.BackgroundColor3 = Color3.new(255, 255, 255)
		textbox2.Name = "TeleportAnswer2"
		local notify = Instance.new("TextLabel", gui)
		notify.Position = UDim2.new(0.085, 0, 0.425, 0)
		notify.Size = UDim2.new(0.15, 0, 0.05, 0)
		notify.Visible = false
		notify.Text = ""
		notify.Font = Enum.Font.SourceSans
		notify.TextScaled = true
		notify.TextSize = 100
		notify.BorderSizePixel = 0
		notify.BorderColor3 = Color3.new(255, 255, 255)
		notify.BackgroundColor3 = Color3.new(255, 255, 255)
		notify.Name = "Notifcation"
	
		button.MouseButton1Click:connect(function(click)
			local find = game.Players:GetChildren()
			for i, find in pairs(find) do
				wait()
				playerSelected = playerSelected + 1
				print("Searching player")
				local telePlayer = find
				if textbox.Text == telePlayer.Name then
					if textbox.Text == player.Name then
						print("Attempt to teleport to self")
						playerSelected = 0
						notify.Visible = true	
						notify.Text = "Cannot use on yourself!"
						wait(1)
						notify.Visible = false
						notify.Text = ""
						return
					else
						print("Success")
						playerSelected = 0
						player.Character.Humanoid.Jump = true
						wait(0.1)
						player.Character:moveTo(telePlayer.Character.HumanoidRootPart.Position)
						notify.Visible = true	
						notify.Text = "Success!"
						wait(1)
						notify.Visible = false
						notify.Text = ""
						return
					end
				else
					if playerSelected < #game.Players:GetChildren() then
						print("Not correct player or invaild name")
					else
						print("Player invaild")
						playerSelected = 0
						notify.Visible = true	
						notify.Text = "Player invaild!"
						wait(1)
						notify.Visible = false
						notify.Text = ""
						return
					end
				end
			end
		end)
	
		button2.MouseButton1Click:connect(function(click)
			local find = game.Players:GetChildren()
			for i, find in pairs(find) do
				wait()
				playerSelected2 = playerSelected2 + 1
				print("Searching player")
				local telePlayer = find
				if textbox2.Text == telePlayer.Name then
					if textbox2.Text == player.Name then
						print("Attempt to teleport to self")
						playerSelected2 = 0
						notify.Visible = true	
						notify.Text = "Cannot use on yourself!"
						wait(1)
						notify.Visible = false
						notify.Text = ""
						return
					else
						print("Success")
						playerSelected2 = 0
						telePlayer.Character.Humanoid.Jump = true
						wait(0.1)
						find.Character:moveTo(player.Character.HumanoidRootPart.Position)
						notify.Visible = true	
						notify.Text = "Success!"
						wait(1)
						notify.Visible = false
						notify.Text = ""
						return
					end
				else
					if playerSelected2 < #game.Players:GetChildren() then
						print("Not correct player or invaild name")
					else
						print("Player invaild")
						playerSelected2 = 0
						notify.Visible = true	
						notify.Text = "Player invaild!"
						wait(1)
						notify.Visible = false
						notify.Text = ""
						return
					end
				end
			end
		end)
	end)
end
coroutine.wrap(ATZPG_fake_script)()
local function KFHPE_fake_script() -- speed10.LocalScript 
	local script = Instance.new('LocalScript', speed10)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = game.Players.LocalPlayer.Character.Humanoid.WalkSpeed + 10
	end)
end
